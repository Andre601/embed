    "seed": "yarn reset --force && prisma seed",
    "generate": "prisma generate",
    "build": "yarn generate && tsc"
  },
  "private": false,
  "dependencies": {
    "prisma-client-lib": "^1.26.5"
  },
  "devDependencies": {
    "prisma": "^1.26.4"
