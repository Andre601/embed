{
  "name": "@widgetbot/database",
  "version": "1.1.3",
  "description": "WidgetBot database client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/widgetbot-io/widgetbot",
  "author": "samdenty99",
  "license": "MIT",
  "scripts": {
    "prisma": "prisma",
    "deploy": "prisma deploy",
    "backup": "prisma export --path backup-data.zip",
    "restore": "prisma import --data backup-data.zip",
