{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/widgetbot-io/server.git",
  "author": "samdd <sam@samdd.me>",
  "scripts": {
    "start": "webpack --colors --config ./scripts/webpack/development --display=minimal",
    "start:node": "ts-node -r tsconfig-paths/register src/server.ts",
    "pull": "git stash && git pull && yarn && yarn build",
    "build": "webpack --colors --config ./scripts/webpack/production --display=minimal",
    "build:new": "rm -rf dist; tsc && ef-tspm && node ./scripts/build",
    "clear:cache": "rm -rf .cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "doc": "typedoc --includeDeclarations --excludeExternals --externalPattern \"**/node_modules/!(@widgetbot)/**\" --ignoreCompilerErrors --out ./docs/ ./src",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "@sindresorhus/is": "^0.11.0",
    "@widgetbot/discord.js": "^12.0.5",
    "@widgetbot/nedb": "^3.0.2",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.1.0-alpha.10",
    "autobind-decorator": "^2.1.0",
    "class-transformer": "^0.1.9",
    "connect-mongo": "^2.0.3",
    "country-emoji": "^1.3.0",
    "doasync": "^2.0.1",
    "express": "^4.16.4",
    "express-interceptor": "^1.2.0",
    "express-rate-limit": "2.14.2",
    "express-session": "^1.15.6",
    "express-winston": "^3.1.0",
    "geoip-lite": "^1.3.6",
    "html-template-tag": "^2.0.0",
    "https-proxy-agent": "^2.2.1",
    "jsonwebtoken": "^8.4.0",
    "memoizee": "^0.4.14",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.11",
    "node-yaml": "^3.1.1",
    "os-name": "^2.0.1",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "pmx": "^1.6.7",
    "pretty-bytes": "^5.1.0",
    "pretty-ms": "^4.0.0",
    "ramda": "^0.26.1",
    "raven": "^2.6.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "routing-controllers": "^0.7.7",
    "timestring": "^5.0.1",
    "tsconfig-paths": "^3.8.0",
    "type-graphql": "^0.16.0",
    "typedi": "^0.8.0",
    "typegoose": "^5.2.1",
    "unseen-data": "^1.0.4",
    "uuid": "^3.3.2",
    "winston": "2.4.4",
    "winston-console-formatter": "^1.0.0-beta.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/scripts/jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|js)"
    ]
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.11",
    "@types/express-winston": "^3.0.1",
    "@types/geoip-lite": "^1.1.29",
    "@types/graphql": "^14.0.6",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/memoizee": "^0.4.2",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.15",
    "@types/nedb": "^1.8.7",
    "@types/passport": "^0.4.6",
    "@types/passport-oauth2": "^1.4.5",
    "@types/ramda": "^0.25.49",
    "@types/raven": "^2.5.2",
    "@types/react-notification-system": "^0.2.39",
    "@types/request": "^2.48.1",
    "@types/uuid": "^3.4.4",
    "@types/webpack-env": "^1.13.7",
    "@types/winston": "2.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "cache-loader": "^1.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^0.1.19",
    "javascript-obfuscator": "0.18.1",
    "jest": "^24.1.0",
    "ncp": "^2.0.0",
    "nodemon-webpack-plugin": "^4.0.7",
    "on-build-webpack": "^0.1.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "typedoc": "^0.14.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
}
